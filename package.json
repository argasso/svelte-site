{
	"name": "svelte-site",
	"version": "0.0.1",
	"scripts": {
		"dev": "npx netlify-cms-proxy-server & vite dev",
		"devv": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"pretypes": "node -v & node --experimental-specifier-resolution=node --harmony --loader ts-node/esm src/lib/cms/configToYml.ts",
		"types": "netlify-ts static/admin/config.yml src/types/netlify-types.ts"
	},
	"devDependencies": {
		"@iconify/svelte": "^2.2.1",
		"@sveltejs/adapter-netlify": "^1.0.0-next.70",
		"@sveltejs/adapter-static": "^1.0.0-next.21",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.5.1",
		"@types/marked": "^4.0.3",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.7",
		"cssnano": "^5.0.8",
		"esbuild-runner": "^2.2.1",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"mdsvex": "^0.10.6",
		"netlify-cms-app": "^2.15.72",
		"netlify-cms-widget-mdx": "^0.5.0",
		"netlify-ts": "^1.0.0",
		"postcss": "^8.4.14",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"rehype-autolink-headings": "^6.1.0",
		"rehype-slug": "^5.0.0",
		"remark-abbr": "^1.4.1",
		"remark-github": "^11.1.0",
		"svelte": "^3.46.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tailwindcss": "^3.1.6",
		"ts-node": "^10.2.1",
		"tslib": "^2.3.1",
		"typescript": "^4.0.0",
		"vite": "^3.0.2"
	},
	"type": "module",
	"dependencies": {
		"@emotion/styled-base": "^10.3.0",
		"@fontsource/fira-mono": "^4.2.2",
		"@netlify/functions": "^1.0.0",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/line-clamp": "^0.4.0",
		"cookie": "^0.5.0",
		"twitter-api-sdk": "^1.1.0"
	}
}
