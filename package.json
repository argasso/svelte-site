{
	"name": "svelte-site",
	"version": "0.0.1",
	"scripts": {
		"dev": "npx netlify-cms-proxy-server & svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"pretypes": "node -v & node --experimental-specifier-resolution=node --harmony --loader ts-node/esm src/lib/cms/configToYml.ts",
		"types": "netlify-ts static/admin/config.yml src/types/netlify-types.ts"
	},
	"devDependencies": {
		"@digitak/esrun": "^1.2.6",
		"@sveltejs/adapter-static": "^1.0.0-next.21",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.4.0",
		"@types/marked": "^3.0.1",
		"@typescript-eslint/eslint-plugin": "^4.19.0",
		"@typescript-eslint/parser": "^4.19.0",
		"autoprefixer": "^10.3.4",
		"cssnano": "^5.0.8",
		"esbuild-runner": "^2.2.1",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-svelte3": "^3.2.0",
		"mdsvex": "^0.9.8",
		"netlify-cms-app": "^2.15.58",
		"netlify-cms-widget-mdx": "^0.4.4",
		"netlify-ts": "^1.0.0",
		"postcss": "^8.3.6",
		"postcss-load-config": "^3.1.0",
		"prettier": "~2.2.1",
		"prettier-plugin-svelte": "^2.2.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-svelte": "^1.0.2",
		"rehype-autolink-headings": "^6.1.0",
		"rehype-slug": "^5.0.0",
		"remark-abbr": "^1.4.1",
		"remark-github": "^11.1.0",
		"svelte": "^3.34.0",
		"svelte-check": "^2.0.0",
		"svelte-preprocess": "^4.9.4",
		"tailwindcss": "^2.2.15",
		"ts-node": "^10.2.1",
		"tslib": "^2.0.0",
		"typescript": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^4.2.2",
		"@lukeed/uuid": "^2.0.0",
		"@tailwindcss/forms": "^0.3.3",
		"@tailwindcss/line-clamp": "^0.2.1",
		"cookie": "^0.4.1"
	}
}